<script>
    // After the window is fully loaded, remove the loading screen and then initialize AOS
    window.addEventListener('load', () => {
        const loadingScreen = document.getElementById('loading-screen');
        
        // Delay hiding the loading screen for at least 30 seconds
        setTimeout(() => {
            loadingScreen.style.display = 'none';
            
            // Initialize AOS after the loading screen is removed
            AOS.init({
                duration: 800,
                delay: 100
            });
        }, 3000); // 30 seconds delay
    });
	
	
	
	
</script>



